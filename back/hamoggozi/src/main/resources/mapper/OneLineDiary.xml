<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hamoggozi.hamoggozi.dao.OneLineDiaryDao">

    <select id="getOneLineDiaryList" parameterType="com.hamoggozi.hamoggozi.dto.OneLineDiaryBean">
        SELECT OD.uid, OD.content, OD.userUid, OD.insertDate, OD.updateDate
        FROM OneLineDiary OD
    </select>

</mapper>



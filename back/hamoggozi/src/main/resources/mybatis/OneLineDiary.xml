<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hamoggozi.hamoggozi.dao.OneLineDiaryMap">

    <select id="getOneLineDiaryList" resultType="oneLineDiary">
        SELECT OD.UID, OD.CONTENT, OD.USERUID
        FROM ONELINEDIARY OD
    </select>

</mapper>